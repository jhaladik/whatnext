{
  "name": "whatnext-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:cloudflare": "next build && npx @cloudflare/next-on-pages@1 --experimental-minify",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "preview": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "deploy": "npx wrangler pages deploy .vercel/output/static --project-name=whatnext-frontend",
    "deploy:direct": "npm run build && npx wrangler pages deploy .vercel/output/static --project-name=whatnext-frontend",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.13",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vercel": "^46.0.1"
  }
}
