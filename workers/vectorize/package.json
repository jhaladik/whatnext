{
  "name": "whatnext-vectorize",
  "version": "1.0.0",
  "description": "TMDB movie vectorization service for What Next recommendation engine",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "init-vectors": "node scripts/initialize-vectors.js",
    "test-rate-limit": "node scripts/test-rate-limits.js",
    "create-vectorize": "wrangler vectorize create whatnext-movies --dimensions=1536 --metric=cosine",
    "create-d1": "wrangler d1 create whatnext-vectorize",
    "init-d1": "wrangler d1 execute whatnext-vectorize --file=./schema.sql",
    "create-kv": "wrangler kv:namespace create RATE_LIMIT_KV && wrangler kv:namespace create PROCESSING_QUEUE"
  },
  "keywords": ["cloudflare", "workers", "vectorize", "tmdb", "recommendations"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.4"
  },
  "dependencies": {
    "openai": "^4.24.1"
  }
}